{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name" : false,
    "max-line-length": false,
    "member-access": false,
    "no-console": false,
    "no-namespace": false,
    "one-line": [
      "check-whitespace",
      "check-open-brace"
    ],
    "quotemark": [
      "single",
      "avoid-escape"
    ],
    "trailing-comma": false,
    "whitespace": [
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-seperator",
      "check-type"
    ],
    "object-literal-sort-keys": false
  }
}